{
  "name": "test",
  "version": "1.0",
  "default_timeout": 10,
  "commands": [
    {
      "command": "scripts/test.sh {{host}} {{v1}} {{v2}} {{v3}}",
      "required": [
        {
          "host": "\\A([0-9]{3}.[0-9]{3}.[0-9]{3}.[0-9]{3}|[0-9A-Za-z._-]+)\\z"
        },
        {
          "v1": "\\A([0-9]{3}.[0-9]{3}.[0-9]{3}.[0-9]{3}|[0-9A-Za-z._-]+)\\z"
        },
        {
          "v2": "\\A([0-9]{3}.[0-9]{3}.[0-9]{3}.[0-9]{3}|[0-9A-Za-z._-]+)\\z"
        }
      ],
      "validate": [
        {
          "v3": "\\A([0-9A-Za-z._-]+)\\z"
        }
      ]
    }
  ]
}